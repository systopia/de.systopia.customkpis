<?xml version="1.0"?>
<extension key="de.systopia.customkpis" type="module">
  <file>customkpis</file>
  <name>Custom KPIs</name>
  <description>Extends the Campaign extension by Systopia with some new KPIs.</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>SYSTOPIA</author>
    <email>info@systopia.de</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/systopia/de.systopia.customkpis</url>
    <url desc="Documentation">https://github.com/systopia/de.systopia.customkpis/blob/master/README.md</url>
    <url desc="Support">https://github.com/systopia/de.systopia.customkpis/issues</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2023-09-26</releaseDate>
  <version>1.2.0</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>5.27</ver>
  </compatibility>
  <requires>
    <ext>de.systopia.campaign</ext>
  </requires>
  <comments>Extends the Campaign extension by Systopia with some new KPIs.</comments>
  <civix>
    <namespace>CRM/Customkpis</namespace>
    <format>23.02.1</format>
  </civix>
  <mixins>
    <mixin>menu-xml@1.0.0</mixin>
    <mixin>smarty-v2@1.0.1</mixin>
  </mixins>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
</extension>
